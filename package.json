{
  "name": "cash-control",
  "version": "1.0.0",
  "description": "Hello World!",
  "main": "./src/main.js",
  "author": "Felipe S. Oliver",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "watch": "webpack --watch",
    "build": "webpack",
    "dev": "concurrently \"npm run watch\" \"npm run start\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.felipe.cash-control",
    "productName": "Cash Control",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "dist/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "src/assets/icon.ico"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "src/assets/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "src/assets/icon.png"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@types/express": "^5.0.0",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "concurrently": "^8.x.x",
    "css-loader": "^7.1.2",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-reloader": "^1.2.3",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.16",
    "wait-on": "^8.0.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.2.0",
    "@mui/material": "^6.1.10",
    "@mui/x-date-pickers": "^7.23.1",
    "better-sqlite3": "^11.7.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^4.21.2",
    "react": "^18.x.x",
    "react-dom": "^18.x.x",
    "uuid": "^11.0.3"
  }
}
